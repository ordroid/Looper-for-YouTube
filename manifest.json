{
  "manifest_version": 3,
  "name": "Looper for YouTube",
  "description": "Loop a certain part of a YouTube video",
  "version": "1.0",
  "permissions": [
	"tabs"
  ],
  "host_permissions": [
	"https://www.youtube.com/*"
  ],
  "action": {
    "default_popup": "form.html",
    "default_icon": "Looper.png"
  },
  "icons": {
	"48": "Looper48.png",
	"128": "Looper128.png"
  }
}